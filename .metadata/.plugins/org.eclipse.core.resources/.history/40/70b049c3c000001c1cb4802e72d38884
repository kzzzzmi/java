package ch03;

public class MainTest3 {

	public static void main(String[] args) {
		
		// 증감 연산자
		// ++, --
		
		int num1 = 1;
		
		// num1 = num1 + 1;
		num1++;
		System.out.println(num1);
		
		int num2 = 1;
		num2--;
		// num2 = num2 - 1;
		System.out.println(num2);
		System.out.println("================");

	}

}

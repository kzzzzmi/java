package ch03;

public class MainTest1 {

	public static void main(String[] args) {
		
		// 대입 연산자 (=)
		int number = 10;
		// 연산에 방향은 오른쪽에서 왼쪽으로 연산됨
		
		int number2 = number;
		// 변수에 변수를 대입할 수도 있다
		
		System.out.println(number);
		System.out.println(number2);
		System.out.println("========================");
		
		// 부호 연산자 (+, -)
		System.out.println(-number);
	}

}

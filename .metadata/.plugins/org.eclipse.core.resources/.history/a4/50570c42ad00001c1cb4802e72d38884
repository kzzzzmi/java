package ch01;

public class CharacterTest {
	// start of main
	public static void main(String[] args) {
		
		char ch1 = 'A';
		System.out.println(ch1);
		System.out.println((int)ch1);
		
		char ch2 = 66;
		System.out.println(ch2);
		System.out.println((int)ch2);
		
		char ch3 = (char)-67;
		System.out.println(ch3);
		
	}	// end of main

}	// end of class

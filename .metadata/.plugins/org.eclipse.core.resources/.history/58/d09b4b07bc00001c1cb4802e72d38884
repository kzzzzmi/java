package ch03;

public class MainTest2 {

	public static void main(String[] args) {

		// 산술 연산자 연습
		int number1 = 5 + 3;
		int number2 = 5 - 2;
		int number3 = 5 * 3;
		int number4 = 5 / 4;
		int number5 = 5 % 4;
		
		System.out.println(number1);
		System.out.println(number2);
		System.out.println(number3);
		System.out.println(number4);
		System.out.println(number5);
	}

}
